<Request>
    <Login>
        <Username>{{username}}</Username>
        <Password >{{password}}</Password>
    </Login>
    <Set operation="add">
        <WebFilterURLGroup>
            <Name>{{ name }} </Name>
            <URLlist>
                {% for url in url_list %}
                <URL>{{ url }}</URL>
                {% endfor %}
            </URLlist>
            <Description>{{ description if description else '' }}</Description>
            <IsDefault>{{ isdefault }}</IsDefault>
        </WebFilterURLGroup>
    </Set>
</Request>