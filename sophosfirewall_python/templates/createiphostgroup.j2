<Request>
   <Login>
        <Username>{{username}}</Username>
        <Password >{{password}}</Password>
    </Login>
    <Set operation="add">    
    <IPHostGroup transactionid="">
		<Name>{{ name }}</Name>
		<IPFamily>IPv4</IPFamily><!-- default IPv4 -->
		<Description>{{ description if description else '' }}</Description>
		<HostList>
            {% for name in host_list %}
			<Host>{{ name }}</Host>
            {% endfor %}
		</HostList>
	</IPHostGroup>  
   </Set>
</Request>